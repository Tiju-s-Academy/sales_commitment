<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_move_commitments" model="ir.cron">
        <field name="name">Move Failed Commitments to Next Day</field>
        <field name="model_id" ref="model_sales_commitment"/>
        <field name="state">code</field>
        <field name="code">model._cron_move_to_next_day()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="active" eval="True"/>
    </record>
</odoo>
